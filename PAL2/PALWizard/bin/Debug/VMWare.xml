    <DESCRIPTION><![CDATA[The amount of overhead memory associated with this virtual machine consumed on the host system.]]></DESCRIPTION>
    <DATASOURCE TYPE="CounterLog" NAME="\VM Memory\Memory Overhead in MB" COLLECTIONVARNAME="CollectionOfVMMemoryMemoryOverheadinMB" EXPRESSIONPATH="\VM Memory\Memory Overhead in MB" TRENDVARNAME="TrendVMMemoryMemoryOverheadinMB" DATATYPE="integer" />
    <CHART CHARTTITLE="\VM Memory\Memory Overhead in MB" ISTHRESHOLDSADDED="False" DATASOURCE="\VM Memory\Memory Overhead in MB" CHARTLABELS="instance" />
