    <DATASOURCE TYPE="CounterLog" NAME="\VM Memory\Memory Reservation in MB" COLLECTIONVARNAME="CollectionOfVMMemoryMemoryReservationinMB" EXPRESSIONPATH="\VM Memory\Memory Reservation in MB" TRENDVARNAME="TrendVMMemoryMemoryReservationinMB" DATATYPE="integer" />
    <CHART CHARTTITLE="\VM Memory\Memory Reservation in MB" ISTHRESHOLDSADDED="False" DATASOURCE="\VM Memory\Memory Reservation in MB" CHARTLABELS="instance" />
  </ANALYSIS>
