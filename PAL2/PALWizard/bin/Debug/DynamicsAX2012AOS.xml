  <ANALYSIS NAME="ServiceModelService 4.0.0.0 Percent Of Max Concurrent Calls" ENABLED="True" CATEGORY="ServiceModelService 4.0.0.0" PRIMARYDATASOURCE="\ServiceModelService 4.0.0.0(*)\Percent Of Max Concurrent Calls" ID="5f74de2e-ac7a-4d09-8d1e-4ba67d97daa2" FROMALLCOUNTERSTATS="False">
    <DATASOURCE TYPE="CounterLog" NAME="\ServiceModelService 4.0.0.0(*)\Percent Of Max Concurrent Calls" COLLECTIONVARNAME="CollectionOfServiceModelService4000PercentOfMaxConcurrentCalls" EXPRESSIONPATH="\ServiceModelService 4.0.0.0(*)\Percent Of Max Concurrent Calls" TRENDVARNAME="TrendServiceModelService4000PercentOfMaxConcurrentCalls" DATATYPE="round2" />
    <CHART CHARTTITLE="\ServiceModelService 4.0.0.0(*)\Percent Of Max Concurrent Calls" ISTHRESHOLDSADDED="True" DATASOURCE="\ServiceModelService 4.0.0.0(*)\Percent Of Max Concurrent Calls" CHARTLABELS="instance">
      <SERIES NAME="Warning">
        <CODE><![CDATA[StaticChartThreshold -CollectionOfCounterInstances $CollectionOfServiceModelService4000PercentOfMaxConcurrentCalls -MinThreshold 70 -MaxThreshold 90 -IsOperatorGreaterThan $True -UseMaxValue $False]]></CODE>
      </SERIES>
      <SERIES NAME="Critical">
        <CODE><![CDATA[StaticChartThreshold -CollectionOfCounterInstances $CollectionOfServiceModelService4000PercentOfMaxConcurrentCalls -MinThreshold 90 -MaxThreshold 100 -IsOperatorGreaterThan $True -UseMaxValue $True]]></CODE>
      </SERIES>
    </CHART>
    <THRESHOLD NAME="MaxConcurrentCalls warning" CONDITION="Warning" COLOR="Yellow" PRIORITY="50">
      <CODE><![CDATA[StaticThreshold -CollectionOfCounterInstances $CollectionOfServiceModelService4000PercentOfMaxConcurrentCalls -Operator 'gt' -Threshold 70]]></CODE>
    </THRESHOLD>
    <THRESHOLD NAME="MaxConcurrentCalls critical" CONDITION="Critical" COLOR="Red" PRIORITY="100">
      <CODE><![CDATA[StaticThreshold -CollectionOfCounterInstances $CollectionOfServiceModelService4000PercentOfMaxConcurrentCalls -Operator 'gt' -Threshold 90
]]></CODE>
    </THRESHOLD>
    <DESCRIPTION><![CDATA[Default MaxConcurrentCalls  is 200.  Increase this value (config file of service group)  in case CPU use is still below 80%,
This value is a percentage of consumed sessions, example 16% means 200 * 0.16 = 32 Calls

Enable counters http://msdn.microsoft.com/en-us/library/ms735098.aspx
Explanation http://blogs.msdn.com/b/sajay/archive/2009/09/14/wcf-performance-counters.aspx
]]></DESCRIPTION>
  </ANALYSIS>
