  <ANALYSIS NAME="ServiceModelService 4.0.0.0 Calls Duration" ENABLED="True" CATEGORY="ServiceModelService 4.0.0.0" PRIMARYDATASOURCE="\ServiceModelService 4.0.0.0(*)\Calls Duration" ID="759ad6c2-17a5-42c6-8761-f66f5a10a5f3" FROMALLCOUNTERSTATS="False">
    <DATASOURCE TYPE="CounterLog" NAME="\ServiceModelService 4.0.0.0(*)\Calls Duration" COLLECTIONVARNAME="CollectionOfServiceModelService4000CallsDuration" EXPRESSIONPATH="\ServiceModelService 4.0.0.0(*)\Calls Duration" TRENDVARNAME="TrendServiceModelService4000CallsDuration" DATATYPE="round3" />
    <CHART CHARTTITLE="\ServiceModelService 4.0.0.0(*)\Calls Duration" ISTHRESHOLDSADDED="True" DATASOURCE="\ServiceModelService 4.0.0.0(*)\Calls Duration" CHARTLABELS="instance">
      <SERIES NAME="Warning">
        <CODE><![CDATA[StaticChartThreshold -CollectionOfCounterInstances $CollectionOfServiceModelService4000CallsDuration -MinThreshold 1 -MaxThreshold 2 -IsOperatorGreaterThan $True -UseMaxValue $False]]></CODE>
      </SERIES>
      <SERIES NAME="Critical">
        <CODE><![CDATA[StaticChartThreshold -CollectionOfCounterInstances $CollectionOfServiceModelService4000CallsDuration -MinThreshold 2 -MaxThreshold 15 -IsOperatorGreaterThan $True -UseMaxValue $True]]></CODE>
      </SERIES>
    </CHART>
    <THRESHOLD NAME="WCF call  time warning" CONDITION="Warning" COLOR="Yellow" PRIORITY="50">
      <CODE><![CDATA[StaticThreshold -CollectionOfCounterInstances $CollectionOfServiceModelService4000CallsDuration -Operator 'gt' -Threshold 1
]]></CODE>
    </THRESHOLD>
    <THRESHOLD NAME="WCF call time Critical" CONDITION="Critical" COLOR="Red" PRIORITY="100">
      <CODE><![CDATA[StaticThreshold -CollectionOfCounterInstances $CollectionOfServiceModelService4000CallsDuration -Operator 'gt' -Threshold 2
]]></CODE>
    </THRESHOLD>
    <DESCRIPTION><![CDATA[]]></DESCRIPTION>
  </ANALYSIS>
